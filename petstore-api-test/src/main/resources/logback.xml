<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<appender name="CONSOLE"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%t] %-5p %C{20}.%L - %m%n</pattern>
		</encoder>
	</appender>

	<root level="Info">
		<appender-ref ref="CONSOLE" />
	</root>

</configuration>